<?xml version="1.0" encoding="UTF-8"?>
<menu>
	<item>
		<title>Boot Android</title>
		<command>console off; fileram -f /romdisk/boot/zImage-android 0x30008000; bootlinux 0x30008000 0x30000100 2456 "console=ttySAC2,115200 androidboot.bootloader=xboot androidboot.hardware=x210"; console on;</command>
	</item>
	<item>
		<title>Update Android System</title>
		<command>fileram -f /romdisk/boot/zImage-initrd 0x30008000; bootlinux 0x30008000 0x30000100 2456 "console=tty0 command=burn-android";</command>
	</item>
	<item>
		<title>Recovery Android System</title>
		<command>fileram -f /romdisk/boot/zImage-initrd 0x30008000; bootlinux 0x30008000 0x30000100 2456 "console=tty0 command=burn-android-recovery";</command>
	</item>
	<item>
		<title>Wipe Android Data and Cache</title>
		<command>fileram -f /romdisk/boot/zImage-initrd 0x30008000; bootlinux 0x30008000 0x30000100 2456 "console=tty0 command=burn-android-wipe";</command>
	</item>
 	<item>
		<title>Linux Debug</title>
		<command>fileram -f /romdisk/boot/zImage-initrd 0x30008000; bootlinux 0x30008000 0x30000100 2456 "console=ttySAC2,115200";</command>
	</item>
	<item>
		<title>Shell Command Line</title>
		<command>exit --shell</command>
	</item>
	<item>
		<title>Reboot System</title>
		<command>reboot</command>
	</item>
	<item>
		<title>Shutdown System</title>
		<command>halt</command>
	</item>
</menu>
